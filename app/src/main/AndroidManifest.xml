<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.apps.xprovpnapps.xpro">

    <uses-permission android:name="com.android.vending.BILLING" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.apps.xprovpnapps.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.apps.xprovpnapps.activity.SubscriptionActivity"/>
        <activity
            android:name="com.apps.xprovpnapps.activity.SplashScreen"
            android:theme="@style/Splashscreentheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.apps.xprovpnapps.activity.ServersActivity"
            android:parentActivityName="com.apps.xprovpnapps.activity.MainActivity" />
        <activity android:name="com.apps.xprovpnapps.activity.MainActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-4752473463552709~1014599317" />


        <activity android:name="com.apps.xprovpnapps.activity.MenuActivity" />
        <activity android:name="com.apps.xprovpnapps.activity.Faq"/>
    </application>

</manifest>